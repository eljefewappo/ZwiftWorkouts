        </SteadyState>
        <SteadyState Duration="60.000004" Power="0.69999999" pace="1428186484" Cadence="85">
            <textevent timeoffset="0" message="Listen to your body, and don&apos;t worry if you need to lower the bias during these workouts to complete."/>
        </SteadyState>
        <SteadyState Duration="90.000008" Power="0.55000001" pace="1428186484" Cadence="85">
            <textevent timeoffset="0" message="Today&apos;s ride is focused on gaining strength through pedaling at 60-70 RPMs"/>
        </SteadyState>
        <SteadyState Duration="180.00002" Power="0.80000001" pace="1428186484" Cadence="65">
