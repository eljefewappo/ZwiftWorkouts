        </SteadyState>
        <SteadyState Duration="10" Power="0.64999998" pace="0"/>
        <SteadyState Duration="10" Power="1.1" pace="0"/>
        <SteadyState Duration="10" Power="0.64999998" pace="0"/>
        <SteadyState Duration="20" Power="1" pace="0"/>
        <SteadyState Duration="15.000001" Power="1.1" pace="0">
