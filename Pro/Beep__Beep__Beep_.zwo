        </SteadyState>
        <SteadyState Duration="180.00002" Power="0.80000001" pace="1428186484" Cadence="65">
            <textevent timeoffset="0" message="The goal is muscular strength during this session rather than a high power and cardio output."/>
        </SteadyState>
        <SteadyState Duration="30.000002" Power="1.05" pace="1428186484" Cadence="85">
            <textevent timeoffset="0" message="REV REV REV"/>
