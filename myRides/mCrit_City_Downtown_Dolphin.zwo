        </SteadyState>
        <SteadyState Duration="15.000001" Power="0.64999998" pace="0"/>
        <SteadyState Duration="20" Power="0.89999998" pace="0"/>
        <SteadyState Duration="15.000001" Power="1.4" pace="0">
